REACT_APP_API_URL=/.netlify/functions

# Feature Flags
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_RISK_CALCULATOR=true

# App Metadata
REACT_APP_NAME=ChartSense
REACT_APP_VERSION=1.0.0
